<Window x:Class="JenningsRE.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
       xmlns:local="clr-namespace:JenningsRE"
       mc:Ignorable="d"
       Title="LoginWindow" Height="450" Width="800">
    <Grid>
        <TextBox Margin="322,178,322,213" TextWrapping="Wrap" FontSize="15" Text="{Binding Username}" Name="logUser"/>
        <TextBox Margin="322,237,322,154" TextWrapping="Wrap" FontSize="15" Text="{Binding Password}" Name="logPass"/>
        <Label Content="Username" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="322,152,0,0"/>
        <Label Content="Password" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="322,212,0,0"/>
        <Label Content="Jennings Real Estate" Margin="295,69,295,0" VerticalAlignment="Top" Height="42" FontSize="20"/>
        <Button Content="Login" VerticalAlignment="Top" Margin="359,290,359,0" Click="Login_Click" IsDefault="true"/>

    </Grid>
</Window>